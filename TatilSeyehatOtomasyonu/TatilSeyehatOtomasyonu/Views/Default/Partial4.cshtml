@using TatilSeyehatOtomasyonu.Models.Siniflar;
@model List<Blog>
<div class="padding1">
    <h4>Favori Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImage" style="width:170px;height:90px;" />
            </div>
            <div class="bot-desc">
                <p> @x.Aciklama.Substring(0, 80)</p>
            </div>
            <div class="clear"></div>
        }

    </div>
    @Html.Action("Partial5", "Default")

</div>